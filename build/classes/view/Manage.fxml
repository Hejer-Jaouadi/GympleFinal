<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ManageController">
   <children>
      <Button fx:id="btnadd" blendMode="SRC_ATOP" layoutX="65.0" layoutY="63.0" mnemonicParsing="false" onAction="#Addbtin" prefHeight="111.0" prefWidth="194.0" style="-fx-border-radius: 10;" text="Add Product">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></Button>
      <Button fx:id="upbtn" layoutX="337.0" layoutY="63.0" mnemonicParsing="false" onAction="#updatebtn" prefHeight="111.0" prefWidth="208.0" style="-fx-background-radius: 10;" text="Update Product">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
         <effect>
            <DropShadow />
         </effect></Button>
      <Button fx:id="delbtn" layoutX="192.0" layoutY="226.0" mnemonicParsing="false" onAction="#deletebtn" prefHeight="111.0" prefWidth="215.0" style="-fx-border-radius: 10;" text="Delete Product">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
         <font>
            <Font name="Arial Black" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
